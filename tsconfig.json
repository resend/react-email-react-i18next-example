{
  "compilerOptions": {
    "composite": false,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    ".react-email"
  ]
}
